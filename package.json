{
  "name": "luc.li",
  "version": "0.0.0-210824",
  "private": true,
  "description": "luc.li",
  "author": "Lucas Lee <me@luc.li>",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "get-fonts": "aws s3 sync s3://luc.li-fonts src/fonts/private",
    "push-fonts": "aws s3 sync src/fonts/private s3://luc.li-fonts",
    "prebuild": "[ -z $DISABLE_PRIVATE_FONTS ] && pnpm get-fonts",
    "build": "pnpm prebuild && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write .",
    "lint:css": "stylelint '**/*.{jsx,tsx,css}'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:format": "prettier --check .",
    "fix:css": "stylelint '**/*.{jsx,tsx,css}' --fix",
    "fix:js": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "husky install",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@reach/router": "^1.3.4",
    "babel-plugin-styled-components": "^2.0.3",
    "date-fns": "^2.28.0",
    "gatsby": "^4.7.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-gtag": "^4.7.0",
    "gatsby-plugin-image": "^2.7.0",
    "gatsby-plugin-manifest": "^4.7.0",
    "gatsby-plugin-mdx": "^3.7.1",
    "gatsby-plugin-page-creator": "^4.7.0",
    "gatsby-plugin-react-helmet": "^5.7.0",
    "gatsby-plugin-remove-trailing-slashes": "^4.7.0",
    "gatsby-plugin-sharp": "^4.7.0",
    "gatsby-plugin-sitemap": "^5.7.0",
    "gatsby-plugin-styled-components": "^5.7.0",
    "gatsby-remark-images": "^6.7.0",
    "gatsby-source-filesystem": "^4.7.0",
    "gatsby-transformer-sharp": "^4.7.0",
    "katex": "^0.16.0",
    "modern-normalize": "^1.1.0",
    "polished": "^4.1.4",
    "postcss": "^8.4.6",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-is": "^17.0.2",
    "react-stickynode": "^4.0.0",
    "rehype-katex": "^5.0.0",
    "remark-math": "^3.0.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^17.0.19",
    "@types/reach__router": "^1.3.10",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-stickynode": "^4.0.0",
    "@types/styled-components": "^5.1.23",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-preset-gatsby": "^2.7.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.5"
  }
}
