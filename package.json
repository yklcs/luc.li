{
  "name": "yklcs.com",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=20",
    "pnpm": ">=7"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "astro dev --host 0.0.0.0",
    "build": "pnpm pull-fonts && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier -w .",
    "pull-fonts": "aws s3 sync s3://yklcs.com-fonts public/fonts/closed",
    "push-fonts": "aws s3 sync public/fonts/closed s3://yklcs.com-fonts",
    "cdk": "tsc -b cdk && cdk"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.0.3",
    "@types/chroma-js": "^2.4.3",
    "astro": "^3.5.0",
    "astro-seo": "^0.8.0",
    "autoprefixer": "^10.4.16",
    "chroma-js": "^2.4.2",
    "date-fns": "^2.30.0",
    "katex": "^0.16.9",
    "motion": "^10.16.4",
    "rehype-katex": "^7.0.0",
    "remark-math": "^5.1.1",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "aws-cdk": "^2.106.0",
    "aws-cdk-lib": "^2.106.0",
    "constructs": "^10.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.1",
    "prettier-plugin-css-order": "^2.0.1",
    "shiki": "^0.14.5",
    "typescript": "^5.2.2"
  }
}
