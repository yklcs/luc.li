{
  "name": "luc.li",
  "version": "0.0.0-210824",
  "private": true,
  "description": "luc.li",
  "author": "Lucas Lee <me@luc.li>",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "develop": "gatsby develop -H 0.0.0.0 -p 8000",
    "start": "gatsby develop -H 0.0.0.0 -p 8000",
    "get-fonts": "aws s3 sync s3://luc.li-fonts src/fonts/private",
    "push-fonts": "aws s3 sync src/fonts/private s3://luc.li-fonts",
    "prebuild": "[ -z $DISABLE_PRIVATE_FONTS ] && pnpm get-fonts",
    "build": "pnpm prebuild && gatsby build",
    "serve": "gatsby serve -H 0.0.0.0 -p 9000",
    "clean": "gatsby clean",
    "format": "prettier --write .",
    "lint:css": "stylelint '**/*.{jsx,tsx,css}'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:format": "prettier --check .",
    "fix:css": "stylelint '**/*.{jsx,tsx,css}' --fix",
    "fix:js": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "husky install",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.2.1",
    "@mdx-js/react": "^2.2.1",
    "@reach/router": "^1.3.4",
    "babel-plugin-styled-components": "^2.0.7",
    "date-fns": "^2.29.3",
    "gatsby": "^5.3.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-gtag": "^5.3.0",
    "gatsby-plugin-image": "^3.3.2",
    "gatsby-plugin-manifest": "^5.3.1",
    "gatsby-plugin-mdx": "^5.3.1",
    "gatsby-plugin-page-creator": "^5.3.1",
    "gatsby-plugin-react-helmet": "^6.3.0",
    "gatsby-plugin-remove-trailing-slashes": "^4.7.0",
    "gatsby-plugin-sharp": "^5.3.2",
    "gatsby-plugin-sitemap": "^6.3.1",
    "gatsby-plugin-styled-components": "^6.3.0",
    "gatsby-remark-images": "^7.3.1",
    "gatsby-source-filesystem": "^5.3.1",
    "gatsby-transformer-sharp": "^5.3.1",
    "katex": "^0.16.4",
    "modern-normalize": "^1.1.0",
    "polished": "^4.2.2",
    "postcss": "^8.4.20",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.7.1",
    "react-is": "^18.2.0",
    "react-stickynode": "^4.1.0",
    "rehype-katex": "5",
    "remark-gfm": "1",
    "remark-math": "4",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^18.11.16",
    "@types/reach__router": "^1.3.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@types/react-stickynode": "^4.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-preset-gatsby": "^3.3.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.9.4"
  }
}
